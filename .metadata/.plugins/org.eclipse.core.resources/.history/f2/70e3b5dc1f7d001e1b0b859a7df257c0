#ifndef INC_STK_PRIVATE_H_
#define INC_STK_PRIVATE_H_

#define STK_BASE_ADDRESS 		0xE000E010

#define STK_CTRL		*((volatile u32*) (STK_BASE_ADDRESS))
#define STK_LOAD		*((volatile u32*) (STK_BASE_ADDRESS+0x04))
#define STK_VAL		*((volatile u32*) (STK_BASE_ADDRESS+0x08))
#define STK_		*((volatile u32*) (STK_BASE_ADDRESS+0x0C))



#endif /* INC_STK_PRIVATE_H_ */
